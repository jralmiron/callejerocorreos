generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model callejero {
  id            Int     @id @default(autoincrement())
  seccion       Int?
  tipo_via      String? @db.VarChar(50)
  nombre_calle  String? @db.VarChar(255)
  numero_inicio Int?
  numero_fin    Int?
  paridad       String? @db.VarChar(1) @default("A") // I=Impares, P=Pares, A=Ambos/Todos
}
